<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Registration page</title>
</head>

<body>
    <center>
        <table border="1" bordercolor="black" cellspacing="0" cellpadding="20">
        <form action="/register" method="post" enctype="multipart/form-data">
                <h1>Register Form</h1>
                <tr>
                    <td>
                        <div>
                            <label><b>First Name</b></label><br>
                            <input type="text" name="fname" placeholder="Enter Your Name" value="<%= values.fname %>">
                            <br>
                            <% if (typeof error1 !=='undefined' && error1.length> 0) { %>
                                <div style="color: red;">
                                    <%= error1 %>
                                </div>
                                <% } %>
                        </div><br>
                        <div>
                            <label><b>Last name</b></label><br>
                            <input type="text" name="lname" placeholder="Enter Your Last Name" value="<%= values.lname %>">
                            <br>
                            <% if (typeof error2 !=='undefined' && error2.length> 0) { %>
                                <div style="color: red;">
                                    <%= error2 %>
                                </div>
                                <% } %>
                        </div><br>
                        <div>
                            <label><b>Gender:</b></label><br>
                            <input type="radio" name="gender" value="male" value="<%= values.gender %>">Male
                            <input type="radio" name="gender" value="female" value="<%= values.gender %>">Female
                            <input type="radio" name="gender" value="others" value="<%= values.gender %>">Others
                            <br>
                            <% if (typeof error6 !=='undefined' && error6.length> 0) { %>
                                <div style="color: red;">
                                    <%= error6 %>
                                </div>
                                <% } %>
                        </div><br>
                        <div>
                            <label><b>Hobby:</b></label><br>
                            <input type="checkbox" value="Dancing" name="hobby" value="<%= values.hobby %>">Dancing
                            <input type="checkbox" value="Reading" name="hobby" value="<%= values.hobby %>">Reading
                            <input type="checkbox" value="Travelling" name="hobby" value="<%= values.hobby %>">Travelling<br>
                            <input type="checkbox" value="Indoor game" name="hobby" value="<%= values.hobby %>">Indoor game
                            <input type="checkbox" value="music" name="hobby" value="<%= values.hobby %>">music
                            <input type="checkbox" value="Bike riding" name="hobby" value="<%= values.hobby %>">Bike riding
                            <br>
                            <% if (typeof error9 !=='undefined' && error9.length> 0) { %>
                                <div style="color: red;">
                                    <%= error9 %>
                                </div>
                                <% } %>
                        </div><br>
                        <div>
                            <label><b>Mobile:</b></label><br>
                            <input type="text" name="phone" placeholder="Enter Your Mobile Number" value="<%= values.phone %>">
                            <br>
                            <% if (typeof error7 !=='undefined' && error7.length> 0) { %>
                                <div style="color: red;">
                                    <%= error7 %>
                                </div>
                                <% } %>
                        </div><br>
                        <div>
                            <label><b>Email:</b></label><br>
                            <input type="Email" name="email" placeholder="Enter Your Email Id" value="<%= values.email %>">
                            <br>
                            <% if (typeof error3 !=='undefined' && error3.length> 0) { %>
                                <div style="color: red;">
                                    <%= error3 %>
                                </div>
                                <% } %>
                        </div><br>
                        <div>
                            <label><b>Possword:</b></label><br>
                            <input type="password" name="password" placeholder="Enter Your Possword" value="<%= values.password %>">
                            <br>
                            <% if (typeof error4 !=='undefined' && error4.length> 0) { %>
                                <div style="color: red;">
                                    <%= error4 %>
                                </div>
                                <% } %>
                        </div><br>
                        <div>
                            <label><b>Confirm Possword:</b></label><br>
                            <input type="password" name="cpassword" placeholder="Confirm Possword" value="<%= values.password %>">
                            <br>
                            <% if (typeof error5 !=='undefined' && error5.length> 0) { %>
                                <div style="color: red;">
                                    <%= error5 %>
                                </div>
                                <% } %>
                        </div><br>
                        <div>
                            <label><b>City:</b></label><br>
                            <select name="city" value="<%= values.city %>">
                                <option>--None--</option>
                                <option>Ahemdabad</option>
                                <option>Rajkot</option>
                                <option>Surat</option>
                                <option>Surendranagar</option>
                                <option>Morbi</option>
                                <option>Bhavnagar</option>
                                <option>Jamnagar</option>
                                <option>Junagadh</option>
                            </select>
                            <br>
                            <% if (typeof error10 !=='undefined' && error10.length> 0) { %>
                                <div style="color: red;">
                                    <%= error10 %>
                                </div>
                                <% } %>
                        </div><br>
                        <div>
                            <label><b>Images:</b></label><br>
                            <input type="file" name="image" value="<%= values.image %>"><br><br>

                            <br>
                            <% if (typeof error8 !=='undefined' && error8.length> 0) { %>
                                <div style="color: red;">
                                    <%= error8 %>
                                </div>
                                <% } %>
                        </div><br>
                        <div>
                            <button type="submit">Submit</button>
                        </div>
                    </td>
                </tr>
            </form>
        </table>
    </center>
</body>

</html>